<input v-on:keyup.enter=\"login()\" v-model=\"email\" class=\"input\" type=\"email\" placeholder=\"Email\">\n                                    <span class=\"icon is-small is-left\">\n                                    <i class=\"fas fa-envelope\"></i>\n                                    </span>\n                                </p>\n                            </div>\n                            <div class=\"field\">\n                                <p class=\"control has-icons-left\">\n                                    <input v-on:keyup.enter=\"login()\" autocomplete v-model=\"password\" class=\"input\" type=\"password\" placeholder=\"Password\">\n                                    <span class=\"icon is-small is-left\">\n                                    <i class=\"fas fa-lock\"></i>\n                                    </span>\n                                </p>\n                            </div>\n                        </form>\n                    </div>\n                    <div class=\"column is-5\">\n                        <p class=\"title is-6\">En quelques clics</p>\n                        <GoogleLogin style=\"margin-bottom: 0.75rem;\"\n                            :params=\"params\"\n                            :renderParams=\"renderParams\"\n                            :onSuccess=\"loginGoogle\"\n                            :onFailure=\"oauthError\">\n                        </GoogleLogin>\n                        <a v-on:click=\"loginFacebook()\" href=\"#\" class=\"fb\"><div class=\"connect\"></div>Sign in with Facebook</a>\n                        <!-- <div v-on:click=\"loginFacebook()\" class=\"fb-login-button\" data-width=\"240\" data-size=\"large\" data-button-type=\"login_with\" data-auto-logout-link=\"false\" data-use-continue-as=\"true\"></div> -->\n                    </div>\n                </div>\n                <div class=\"notification is-danger\" v-if=\"loginStatus === 1\">\n                    Veuillez rentrer vos identifiants.\n                </div>\n                <div class=\"notification is-danger\" v-if=\"loginStatus === 2\">\n                    E-mail et/ou mot de passe incorrect.\n                </div>\n                <div class=\"notification is-danger\" v-if=\"loginStatus === 3\">\n                    Une erreur est survenue.\n                </div>\n            </section>\n            <footer class=\"modal-card-foot\">\n                <button class=\"button is-primary\" v-on:click=\"login()\">Se connecter</button>\n                <button class=\"button\" v-on:click=\"close()\">Annuler</button>\n            </footer>\n        </div>\n        <button class=\"modal-close is-large\" aria-label=\"close\" @click=\"close()\"></button>\n    </div>\n</template>\n\n<script>\nimport Vue from 'vue';\nimport GoogleLogin from 'vue-google-login';\n\nconst   PENDING = 0;\nconst   ERROR_ARGS = 1;\nconst   WRONG_CREDENTIALS = 2;\nconst   GOOGLE_ERROR = 3;\nconst   ALREADY_CONNECTED = 4;\n\nexport default {\n  name: 'Login',\n  components: {\n    GoogleLogin\n  },\n  props: {\n      state: {\n          type: String,\n          default: ''\n      }\n  },\n  data() {\n      return {\n          email: \"\",\n          password: \"\",\n          loginStatus: PENDING,\n          params: {\n                client_id: \"1083183077543-ogjph9laolcpm0uphqk3d0j7u7pf13i5.apps.googleusercontent.com\"\n            },\n            renderParams: {\n                width: 240,\n                height: 40,\n                longtitle: true\n            }\n      }\n  },\n  methods: {\n      close() {\n          this.$emit('update-state', '');\n      },\n      login() {\n            this.axios.post(`http://localhost:8080/login?email=${this.email}&password=${this.password}/`)\n            .then((response) => {\n                if (response.data === \"E-mail or password doesn't exist.\") {\n                    this.loginStatus = ERROR_ARGS;\n                    return;\n                } else if (response.data === \"No user associated.\") {\n                    this.loginStatus = WRONG_CREDENTIALS;\n                    return;\n                } else {\n                    localStorage.setItem(\"authToken\", response.data);\n                    this.close();\n                }\n            })\n      },\n      loginGoogle(googleUser) {\n            var id = googleUser.getId();\n\n            this.axios.post(`http://localhost:8080/register/tpa?id_tpa=${id}/`)\n            .then((response) => {\n                if (response.data === \"Token doesn't exist.\") {\n                    this.loginStatus = ERROR_ARGS;\n                    return;\n                } else {\n                    localStorage.setItem(\"authToken\", response.data);\n                    this.close();\n                }\n            })\n      },\n      loginFacebook() {\n          var closeModal = this.close;\n          var loginState = this.loginStatus;\n          var loginResponse;\n\n          Vue.FB.getLoginStatus(function (response) {\n              if (response.status === \"connected\") {\n                  loginState = ALREADY_CONNECTED;\n                  loginResponse = response;\n              }\n          });\n          if (loginState === ALREADY_CONNECTED) {\n                this.axios.post(`http://localhost:8080/register/tpa?id_tpa=${loginResponse.authResponse.userID}/`)\n                .then((response) => {\n                    if (response.data === \"Token doesn't exist.\") {\n                        this.loginStatus = ERROR_ARGS;\n                        return;\n                    } else {\n                        localStorage.setItem(\"authToken\", response.data);\n                        closeModal();\n                    }\n                })\n                return;\n          }\n          Vue.FB.login(function (response) {\n              if (response.status === \"connected\") {\n                this.axios.post(`http://localhost:8080/register/tpa?id_tpa=${response.authResponse.userID}/`)\n                .then((response) => {\n                    if (response.data === \"Token doesn't exist.\") {\n                        this.loginStatus = ERROR_ARGS;\n                        return;\n                    } else {\n                        localStorage.setItem(\"authToken\", response.data);\n                        closeModal();\n                    }\n                })\n              } else {\n                  loginState = GOOGLE_ERROR;\n              }\n          });\n      },\n      oauthError() {\n          this.loginStatus = GOOGLE_ERROR;\n      }\n  }\n}\n</script>\n\n<style>\n\ndiv {\n    -webkit-column-rule: 4px double #ff00ff; /* Chrome, Safari, Opera */\n  -moz-column-rule: 4px double #ff00ff; /* Firefox */\n  column-rule: 4px double #ff00ff;\n}\n\na.fb {\n    font-family: Lucida Grande, Helvetica Neue, Helvetica, Arial, sans-serif;\n    display: inline-block;\n    font-size: 14px;\n    padding: 13px 30px 15px 44px;\n    background: #3A5A97;\n    color: #fff;\n    text-shadow: 0 -1px 0 rgba(0,0,20,.4);\n    text-decoration: none;\n    line-height: 1;\n    position: relative;\n    border-radius: 5px;\n    width: 240px;\n    text-align: center;\n}\n\n.connect {\n    float: left;\n}\n\n.connect:before {\n    display: inline-block;\n    position: relative;\n    background-image: url(data:image/png;base64,);\n    height: 23px;\n    background-repeat: no-repeat;\n    background-position: 0px 2px;\n    text-indent: -9999px;\n    text-align: center;\n    width: 29px;\n    line-height: 23px;\n    margin: -8px 29px -7px -35px;\n    padding: 2 25px 0 0;\n    content: \"f\";\n}\n\n</style>"]}]}